{
  "name": "@verndale/core",
  "version": "0.0.1",
  "description": "Collection of JavaScript helpers meant to be used for core development.",
  "license": "MIT",
  "main": "lib/index.js",
  "author": {
    "name": "Joe Fusco",
    "email": "joe.fusco@verndale.com"
  },
  "scripts": {
    "version": "git add .",
    "postversion": "git push origin master --tags",
    "status": "flow status",
    "lint": "eslint --fix --color --debug --ignore-path .eslintignore ./core/*.js",
    "test": "npm run lint && karma start --single-run --no-auto-watch",
    "build": "rimraf ./docs ./lib && npm test && npm run status && documentation build dom/* core/* -f html -g true -o docs && npx babel core --out-dir lib"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
