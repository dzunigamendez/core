{
  "name": "@verndale/core",
  "version": "0.2.0",
  "description": "Collection of JavaScript helpers meant to be used for core development.",
  "license": "MIT",
  "main": "lib/index.js",
  "author": {
    "name": "Joe Fusco",
    "email": "joe.fusco@verndale.com"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/v3rndale/core.git"
  },
  "scripts": {
    "preversion": "rimraf ./docs ./lib && npm test",
    "version": "npm run build && git add .",
    "postversion": "git push origin master --tags",
    "status": "flow status",
    "lint": "eslint --fix --color --debug --ignore-path .eslintignore ./src/*.js",
    "test": "npm run status && npm run lint && karma start --single-run --no-auto-watch",
    "build": "documentation build dom/* src/* -f html -g true -o docs && npx babel src --out-dir lib"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.5",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-runtime": "6.26.0",
    "documentation": "8.0.0",
    "eslint": "5.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "flow-bin": "0.75.0",
    "isparta-loader": "2.0.0",
    "istanbul": "0.4.5",
    "istanbul-instrumenter-loader": "3.0.1",
    "jasmine-core": "3.1.0",
    "jasmine-jquery": "2.1.1",
    "jquery": "3.3.1",
    "karma": "^2.0.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-safari-launcher": "1.0.0",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "3.0.0",
    "minimist": "1.2.0",
    "phantomjs": "2.1.7",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": ">=1.9",
    "rimraf": "2.6.2",
    "run-sequence": "2.2.1",
    "webpack": "4.12.1"
  },
  "keywords": [
    "import",
    "webpack",
    "chunking",
    "code-splitting",
    "es6",
    "dynamic-import"
  ]
}
